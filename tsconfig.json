{
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "rootDir": "./ts/",
    "target": "ES2017",
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": ["./ts"]
}
